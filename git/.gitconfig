[core]
  editor = nano

[fetch]
  prune = true

[rebase]
  autoStash = true

[help]
  # If autocorrect comes up with only one suggestion, automatically run it after
  # a delay (in decaseconds)
  autoCorrect = 20

[include]
  path = ~/dotfiles/git/shopify
  path = ~/dotfiles/git/aliases

[diff]
  algorithm = patience

[protocol]
  version = 2
